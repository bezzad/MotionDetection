<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Epoxy.Wpf" Version="1.10.0" />
    <PackageReference Include="FlashCap" Version="1.6.0" />
    <PackageReference Include="SkiaImageView.Wpf" Version="1.5.0" />
    <PackageReference Include="SkiaSharp" Version="2.88.5" />
    <PackageReference Include="SkiaSharp.NativeAssets.Linux" Version="2.88.5" />
    <PackageReference Include="SkiaSharp.NativeAssets.macOS" Version="2.88.5" />
    <PackageReference Include="SkiaSharp.NativeAssets.Win32" Version="2.88.5" />
    <PackageReference Include="SkiaSharp.Views.WPF" Version="2.88.5" />
  </ItemGroup>

</Project>
